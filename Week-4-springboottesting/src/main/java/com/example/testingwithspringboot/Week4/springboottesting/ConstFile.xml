<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
 
    <!-- bean definitions here -->
    <bean class="BankAdmin" id ="admin1">
    	<!-- Dependency injection using Setter Methods -->
    	<property name="adminName" value="Sr.Rock"></property>
    	<property name="adminId" value="005"></property>
    </bean>
    
    	<bean class="BankEmployee" id="bank1">	
    	<!-- Dependency injection using constructor -->
         <constructor-arg value="Jhon Weak" ></constructor-arg>
         <constructor-arg value="true"></constructor-arg>
    </bean>
 
</beans>